{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "declaration": false,
    "noEmit": true,
    "lib": ["DOM", "ES6"],
    "esModuleInterop": true,
    "strict": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": false
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts"]
}
